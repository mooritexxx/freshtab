{
  "manifest_version": 3,
  "name": "Personalized New Tab",
  "version": "2.7.0", // Or whatever you want the main version to be for this state
  "description": "Displays weather, time, city events, and more on your new tab page.",
  "permissions": [
    "geolocation",
    "storage",
    "topSites" 
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html" 
  },
  "options_page": "options.html",
  "action": {
    "default_title": "Personalized New Tab Settings",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}