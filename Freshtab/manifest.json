{
  "manifest_version": 3,
  "name": "Freshtab",
  "version": "3.0.0",
  "description": "Displays weather, time, city events, and more on your new tab page.",
  "permissions": [
    "geolocation",
    "storage",
    "topSites"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_title": "Freshtab",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}